            </field>
            <field name="image" type="base64" file="clash_of_clans/static/img/giant.PNG"/>
        </record>

        <!-- Goblin -->
        <record id="troop_goblin" model="clash.of.clans.troops">
            <field name="name">Goblin</field>
            <field name="level">1</field>
            <field name="damage_per_second">11</field>
            <field name="hit_points">12</field>
            <field name="training_cost">10</field>
            <field name="training_time">3</field>
            <field name="favourite_target">any</field>
            <field name="damage_type">single</field>
            <field name="targets">ground</field>
            <field name="housing_space">1</field>
            <field name="movement_speed">50</field>
