            <field name="image" type="base64" file="clash_of_clans/static/img/Wall_Breaker.webp"/>
        </record>

        <!-- Wizard -->
        <record id="troop_wizard" model="clash.of.clans.troops">
            <field name="name">Wizard</field>
            <field name="level">1</field>
            <field name="damage_per_second">50</field>
            <field name="hit_points">13</field>
            <field name="training_cost">60</field>
            <field name="training_time">15</field>
            <field name="favourite_target">any</field>
            <field name="damage_type">area</field>
            <field name="targets">both</field>
            <field name="housing_space">4</field>
            <field name="movement_speed">16</field>
            <field name="description">The wizard is a terrifying presence on the battlefield.</field>
