        <record id="troop_giant" model="clash.of.clans.troops">
            <field name="name">Giant</field>
            <field name="level">1</field>
            <field name="damage_per_second">11</field>
            <field name="hit_points">90</field>
            <field name="training_cost">30</field>
            <field name="training_time">27</field>
